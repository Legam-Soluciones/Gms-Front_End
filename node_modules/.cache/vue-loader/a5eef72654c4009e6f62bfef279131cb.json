v class=\"my-2\">\n                <v-btn\n                  class=\"ma-1 white--text\"\n                  text\n                  color=\"secondary\"\n                  @click=\"Inicio()\"\n                >\n                  <v-icon> home </v-icon> Inicio</v-btn\n                >\n              </div>\n            </v-list-item-action>\n          </v-list-item>\n          <v-row align=\"center\">\n            <v-col class=\"text-center\" cols=\"12\" sm=\"4\">\n              <div class=\"my-2\">\n                <v-btn\n                  class=\"ma-1 white--text\"\n                  text\n                  color=\"primary\"\n                  @click=\"Agenda()\"\n                >\n                  <v-icon> date_range </v-icon> Agenda</v-btn\n                >\n              </div>\n\n              <div class=\"my-2\">\n                <v-btn\n                  class=\"ma-1 white--text\"\n                  text\n                  color=\"primary\"\n                  @click=\"expedientes()\"\n                >\n                  <v-icon> masks </v-icon> Expedientes</v-btn\n                >\n              </div>\n              <div class=\"my-2\">\n                <v-btn\n                  class=\"ma-1 white--text\"\n                  text\n                  color=\"primary\"\n                  @click=\"documentos()\"\n                  ><v-icon> assignment </v-icon> Documentos\n                </v-btn>\n              </div>\n              <div class=\"my-2\">\n                <v-btn\n                  class=\"ma-1 white--text\"\n                  text\n                  color=\"primary\"\n                  @click=\"finanzas()\"\n                  ><v-icon> local_atm </v-icon> Finanzas\n                </v-btn>\n              </div>\n              <div class=\"my-2\">\n                <v-btn\n                  class=\"ma-1 white--text\"\n                  text\n                  color=\"primary\"\n                  @click=\"tareas()\"\n                  ><v-icon> check_box </v-icon> Tareas\n                </v-btn>\n              </div>\n              <div class=\"my-2\">\n                <v-btn\n                  class=\"ma-1 white--text\"\n                  text\n                  color=\"primary\"\n                  href=\"https://web.whatsapp.com/\"\n                  ><v-icon> sms </v-icon> Mensajes\n                </v-btn>\n              </div>\n              <div class=\"my-2\">\n                <v-btn\n                  class=\"ma-1 white--text\"\n                  text\n                  color=\"primary\"\n                  @click=\"estadisticas()\"\n                  ><v-icon> insights </v-icon> Estadísticas\n                </v-btn>\n              </div>\n              <div class=\"my-2\">\n                <v-btn class=\"ma-1 white--text\" text color=\"error\"\n                  ><v-icon> exit_to_app</v-icon> Salir\n                </v-btn>\n              </div>\n            </v-col>\n          </v-row>\n        </template>\n      </v-list>\n    </v-navigation-drawer>\n    <nav>\n      <v-navigation-drawer\n        v-model=\"drawer2\"\n        dark\n        app\n        class=\"accent-2\"\n        color=\"#007096\"\n        fixed\n        right\n      >\n        <v-layout column>\n          <v-flex class=\"mt-5\">\n            <div class=\"d-flex justify-center\">\n              <v-avatar size=\"100\">\n                <img src=\"@/assets/logo.png\" alt=\"\" />\n              </v-avatar>\n            </div>\n          </v-flex>\n        </v-layout>\n        <p class=\"white--text subheading m-1 text-center\">\n          Más Salud Slim Clinic\n        </p>\n        <v-list rounded dense>\n          <v-list-item router to=\"/login\">\n            <v-list-item-action>\n              1\n            </v-list-item-action>\n            <v-list-item-content>\n              <v-list-item-title>\n                Mi Cuenta\n              </v-list-item-title>\n            </v-list-item-content>\n          </v-list-item>\n\n          <v-list-item router to=\"/\">\n            <v-list-item-action>\n              2\n            </v-list-item-action>\n            <v-list-item-content>\n              <v-list-item-title @click=\"logout\">\n                Salir\n              </v-list-item-title>\n            </v-list-item-content>\n          </v-list-item>\n        </v-list>\n      </v-navigation-drawer>\n    </nav>\n\n    <v-app-bar\n      :clipped-left=\"$vuetify.breakpoint.lgAndUp\"\n      app\n      color=\"#007096\"\n      dark\n    >\n      <v-app-bar-nav-icon @click.stop=\"drawer = !drawer\"></v-app-bar-nav-icon>\n      <v-toolbar-title style=\"width: 300px\" class=\"ml-0 pl-4\">\n        <span class=\"hidden-sm-and-down\"> </span>\n      </v-toolbar-title>\n      <v-spacer></v-spacer>\n      <v-icon @click.stop=\"drawer2 = !drawer2\">apps</v-icon>\n    </v-app-bar>\n    <v-main>\n      <v-container class=\"fill-height\" fluid>\n        <v-slide-y-transition>\n          <router-view />\n        </v-slide-y-transition>\n\n        <v-row align=\"center\" justify=\"center\">\n          <v-tooltip right>\n            <template v-slot:activator=\"{ on }\">\n              <v-btn icon large target=\"_blank\" v-on=\"on\">\n                <v-icon large>mdi-code-tags</v-icon>\n              </v-btn>\n            </template>\n            <span>Source</span>\n          </v-tooltip>\n        </v-row>\n      </v-container>\n\n      <v-footer dark height=\"auto\">\n        <v-layout justify-center>\n          <v-flex text-xs-center>\n            <v-card flat item color=\"primary\" class=\"white--text\">\n              <v-card-text class=\" text-center white--text pt-0\">\n                MásSaludSlimClinic &copy;2020\n              </v-card-text>\n            </v-card>\n          </v-flex>\n        </v-layout>\n      </v-footer>\n    </v-main>\n  </v-app>\n</template>\n\n<script>\nimport firebase from \"firebase\";\nexport default {\n  data: () => ({\n    drawer: false,\n    drawer2: false,\n    isOpen: false,\n    user: null\n  }),\n  methods: {\n    // Cada uno de estos documentos lleva a su sección correspondiente.\n    Agenda() {\n      this.$router.push({ name: \"Agenda\" });\n    },\n    Inicio() {\n      this.$router.push({ name: \"HelloWorld\" });\n    },\n    documentos() {\n      this.$router.push({ name: \"documentos\" });\n    },\n    expedientes() {\n      this.$router.push({ name: \"expedientes\" });\n    },\n    tareas() {\n      this.$router.push({ name: \"tareas\" });\n    },\n    estadisticas() {\n      this.$router.push({ name: \"estadisticas\" });\n    },\n    finanzas() {\n      this.$router.push({ name: \"finanzas\" });\n    },\n    toggleMenu() {\n      const status = !this.isOpen;\n      this.isOpen = status;\n    },\n    logout() {\n      firebase\n        .auth()\n        .signOut()\n        .then(() => {\n          this.$router.push({ name: \"login\" });\n        });\n    }\n  },\n  created() {\n    firebase.auth().onAuthStateChanged(user => {\n      if (user) {\n        this.user = user;\n      } else {\n        this.user = null;\n      }\n    });\n  }\n};\n</script>\n"]}]}