\r\n  <v-row>\r\n    <v-col cols=\"12\" sm=\"6\">\r\n      <v-hover v-slot:default=\"{ hover }\" open-delay=\"200\">\r\n        <v-card\r\n          :elevation=\"hover ? 16 : 2\"\r\n          class=\"mx-auto\"\r\n          height=\"700\"\r\n          max-width=\"500\"\r\n        >\r\n          <v-card-text class=\"font-weight-medium mt-12 text-center subtitle-1\">\r\n            <h3>Busqueda expedientes</h3>\r\n            <br />\r\n            <v-divider></v-divider>\r\n            <v-card class=\"mx-auto\" max-width=\"400\" tile> </v-card>\r\n            <template>\r\n              <v-form>\r\n                <v-container>\r\n                  <v-row>\r\n                    <v-col cols=\"12\" sm=\"6\">\r\n                      <v-text-field\r\n                        v-model=\"cedula\"\r\n                        label=\"Cédula\"\r\n                        shaped\r\n                        filled\r\n                      ></v-text-field>\r\n                    </v-col>\r\n                  </v-row>\r\n                  <v-row>\r\n                    <v-col>\r\n                      <v-btn outlined color=\"primary\" dark @click=\"created\"\r\n                        >Ver Expedientes</v-btn\r\n                      >\r\n                    </v-col>\r\n                  </v-row>\r\n                  <v-row>\r\n                    <v-col>\r\n                      <Addexpedientes />\r\n                    </v-col>\r\n                  </v-row>\r\n                </v-container>\r\n              </v-form>\r\n            </template>\r\n          </v-card-text>\r\n        </v-card>\r\n      </v-hover>\r\n    </v-col>\r\n\r\n    <v-col cols=\"12\" sm=\"6\">\r\n      <v-hover v-slot:default=\"{ hover }\" close-delay=\"200\">\r\n        <v-card\r\n          :elevation=\"hover ? 16 : 2\"\r\n          class=\"d-flex align-center justify-center mx-auto\"\r\n          height=\"700\"\r\n          max-width=\"1000\"\r\n          color=\"grey lighten-3\"\r\n        >\r\n          <v-card\r\n            class=\"d-flex justify-center mb-6\"\r\n            outlined\r\n            tile\r\n            height=\"250\"\r\n            width=\"450\"\r\n            center\r\n          >\r\n            <div\r\n              class=\"text-center \"\r\n              v-for=\"Expediente in Expedientes\"\r\n              :key=\"Expediente.IDExpediente\"\r\n            >\r\n              <div>\r\n                <v-icon>person</v-icon>\r\n                <br />\r\n                {{ Expediente.PrimerNombre }}\r\n                {{ Expediente.SegundoNombre }}\r\n                {{ Expediente.PrimerApellido }}\r\n                {{ Expediente.SegundoApellido }}\r\n              </div>\r\n              <div>\r\n                <v-icon>credit_card</v-icon>\r\n                <br />Cédula:\r\n                {{ Expediente.NumeroIdentificacion }}\r\n              </div>\r\n              <div>\r\n                <v-icon>phone</v-icon>\r\n                <br />Teléfono: {{ Expediente.Telefono1 }}\r\n              </div>\r\n              <div>\r\n                <v-icon>mail</v-icon><br />\r\n                Email: {{ Expediente.Email }}\r\n              </div>\r\n              <div>\r\n                <template>\r\n                  <v-dialog max-height=\"1200px\" width=\"700px\">\r\n                    <template v-slot:activator=\"{ on, attrs }\">\r\n                      <v-btn\r\n                        outlined\r\n                        color=\"primary\"\r\n                        dark\r\n                        v-bind=\"attrs\"\r\n                        v-on=\"on\"\r\n                      >\r\n                        Ver todo el expediente\r\n                      </v-btn>\r\n                    </template>\r\n                    <v-card\r\n                      :elevation=\"hover ? 16 : 2\"\r\n                      class=\"d-flex align-center justify-center mx-auto\"\r\n                      height=\"900\"\r\n                      max-width=\"1000\"\r\n                      color=\"grey lighten-3\"\r\n                    >\r\n                      <v-card\r\n                        class=\"d-flex justify-center mb-6\"\r\n                        outlined\r\n                        tile\r\n                        height=\"650\"\r\n                        width=\"550\"\r\n                        center\r\n                      >\r\n                        <div\r\n                          class=\"text-center \"\r\n                          v-for=\"Expediente in Expedientes\"\r\n                          :key=\"Expediente.IDExpediente\"\r\n                        >\r\n                          <div>\r\n                            <br />\r\n                            <v-icon>person</v-icon>\r\n                            {{ Expediente.PrimerNombre }}\r\n                            {{ Expediente.SegundoNombre }}\r\n                            {{ Expediente.PrimerApellido }}\r\n                            {{ Expediente.SegundoApellido }}\r\n                          </div>\r\n                          <div>\r\n                            <br />\r\n                            <v-icon>elderly</v-icon>\r\n                            Edad:\r\n                            {{ Expediente.Edad }}\r\n                          </div>\r\n                          <div>\r\n                            <br />\r\n                            <v-icon>date_range</v-icon>\r\n                            Fecha de Nacimiento:\r\n                            {{ Expediente.FechaNacimiento }}\r\n                            <br />\r\n                            Fecha de Creado:\r\n                            {{ Expediente.FechaCreado }}\r\n                          </div>\r\n                          <div>\r\n                            <br />\r\n                            <v-icon>face</v-icon>\r\n                            Género:\r\n                            {{ Expediente.Genero }}\r\n                          </div>\r\n                          <div>\r\n                            <br />\r\n                            <v-icon>card_membership</v-icon>\r\n                            ID del Expediente:\r\n                            {{ Expediente.IDExpediente }}\r\n                            <br />\r\n                            ID (Tipo de Identificación):\r\n                            {{ Expediente.IDTipoIdentificacion }}\r\n                          </div>\r\n                          <div>\r\n                            <br />\r\n                            Nombre del Convenio:\r\n                            {{ Expediente.NombreConvenio }}\r\n                          </div>\r\n                          <div>\r\n                            <br />\r\n                            <v-icon>attach_money</v-icon>\r\n                            Saldo:\r\n                            {{ Expediente.Saldo }}\r\n                          </div>\r\n                          <div>\r\n                            <br />\r\n                            <v-icon>credit_card</v-icon>\r\n                            Cédula:\r\n                            {{ Expediente.NumeroIdentificacion }}\r\n                          </div>\r\n                          <div>\r\n                            <br />\r\n                            <v-icon>phone</v-icon>\r\n                            Teléfono:\r\n                            {{ Expediente.Telefono1CodigoPais }}\r\n                            {{ Expediente.Telefono1 }}\r\n                            <br />\r\n                            Teléfono secundario:\r\n                            {{ Expediente.Telefono2CodigoPais }}\r\n                            {{ Expediente.Telefono2 }}\r\n                          </div>\r\n                          <div>\r\n                            <br />\r\n                            <v-icon>mail</v-icon>\r\n                            Email: {{ Expediente.Email }}\r\n                          </div>\r\n                          <div>\r\n                            <v-btn\r\n                              outlined\r\n                              color=\"primary\"\r\n                              dark\r\n                              class=\"mt-4 mb-4 mr-4\"\r\n                              @click=\"deleted(Expediente)\"\r\n                              >Eliminar Expediente</v-btn\r\n                            >\r\n                            <template>\r\n                              <v-dialog max-height=\"1200px\" width=\"700px\">\r\n                                <template v-slot:activator=\"{ on, attrs }\">\r\n                                  <v-btn\r\n                                    v-bind=\"attrs\"\r\n                                    v-on=\"on\"\r\n                                    outlined\r\n                                    color=\"primary\"\r\n                                    class=\"mt-4 mb-4\"\r\n                                  >\r\n                                    Actualizar Expedientes\r\n                                  </v-btn>\r\n                                </template>\r\n                                <v-card max-height=\"1200px\" width=\"800px\">\r\n                                  <v-container>\r\n                                    <v-form>\r\n                                      <v-text-field\r\n                                        v-model=\"PrimerNombre\"\r\n                                        type=\"text\"\r\n                                        label=\"Primer Nombre\"\r\n                                      ></v-text-field>\r\n                                      <v-text-field\r\n                                        v-model=\"SegundoNombre\"\r\n                                        type=\"text\"\r\n                                        label=\"Segundo Nombre\"\r\n                                      ></v-text-field>\r\n                                      <v-text-field\r\n                                        v-model=\"PrimerApellido\"\r\n                                        type=\"text\"\r\n                                        label=\"Primer Apellido\"\r\n                                      ></v-text-field>\r\n                                      <v-text-field\r\n                                        v-model=\"SegundoApellido\"\r\n                                        type=\"text\"\r\n                                        label=\"Segundo Apellido\"\r\n                                      ></v-text-field>\r\n                                      <v-select\r\n                                        :items=\"[\r\n                                          'Cédula de Identidad',\r\n                                          'Extranjero',\r\n                                          'DIMEX',\r\n                                          'Menor de Edad'\r\n                                        ]\"\r\n                                        label=\"Seleccionar Identificación\"\r\n                                        v-model=\"IDTipoIdentificacion\"\r\n                                      ></v-select>\r\n                                      <v-text-field\r\n                                        v-model=\"NumeroIdentificacion\"\r\n                                        type=\"number\"\r\n                                        label=\"Identificación\"\r\n                                      ></v-text-field>\r\n                                      <v-select\r\n                                        :items=\"[\r\n                                          'Femenino',\r\n                                          'Masculino',\r\n                                          'No especifica'\r\n                                        ]\"\r\n                                        label=\"Género\"\r\n                                        v-model=\"Genero\"\r\n                                      ></v-select>\r\n                                      <v-text-field\r\n                                        v-model=\"FechaNacimiento\"\r\n                                        type=\"date\"\r\n                                        label=\"Fecha de nacimiento\"\r\n                                      ></v-text-field>\r\n                                      <v-text-field\r\n                                        v-model=\"Edad\"\r\n                                        type=\"number\"\r\n                                        label=\"Edad\"\r\n                                      ></v-text-field>\r\n                                      <v-text-field\r\n                                        v-model=\"Telefono1CodigoPais\"\r\n                                        type=\"number\"\r\n                                        label=\"Código de país\"\r\n                                      ></v-text-field>\r\n                                      <v-text-field\r\n                                        v-model=\"Telefono1\"\r\n                                        type=\"number\"\r\n                                        label=\"Teléfono\"\r\n                                      ></v-text-field>\r\n                                      <v-text-field\r\n                                        v-model=\"Telefono2CodigoPais\"\r\n                                        type=\"number\"\r\n                                        label=\"Código de país 2\"\r\n                                      ></v-text-field>\r\n                                      <v-text-field\r\n                                        v-model=\"Telefono2\"\r\n                                        type=\"number\"\r\n                                        label=\"Teléfono secundario\"\r\n                                      ></v-text-field>\r\n                                      <v-text-field\r\n                                        v-model=\"Email\"\r\n                                        type=\"text\"\r\n                                        label=\"Correo Electrónico\"\r\n                                      ></v-text-field>\r\n                                      <v-btn\r\n                                        color=\"primay\"\r\n                                        class=\"mr-4\"\r\n                                        @click=\"update(Expediente)\"\r\n                                        >Actualizar Expediente</v-btn\r\n                                      >\r\n                                    </v-form>\r\n                                  </v-container>\r\n                                </v-card>\r\n                              </v-dialog>\r\n                            </template>\r\n                          </div>\r\n                        </div>\r\n                      </v-card>\r\n                    </v-card>\r\n                  </v-dialog>\r\n                </template>\r\n              </div>\r\n            </div>\r\n          </v-card>\r\n        </v-card>\r\n      </v-hover>\r\n    </v-col>\r\n  </v-row>\r\n</template>\r\n\r\n<script>\r\nimport { db } from \"@/main\";\r\nimport Addexpedientes from \"./agregarexpedientes.vue\";\r\n\r\nexport default {\r\n  components: {\r\n    Addexpedientes\r\n  },\r\n  data() {\r\n    return {\r\n      PrimerNombre: null,\r\n      SegundoNombre: null,\r\n      PrimerApellido: null,\r\n      SegundoApellido: null,\r\n      IDTipoIdentificacion: null,\r\n      NumeroIdentificacion: null,\r\n      Genero: null,\r\n      FechaNacimiento: null,\r\n      Edad: null,\r\n      Telefono1CodigoPais: null,\r\n      Telefono2CodigoPais: null,\r\n      Telefono1: null,\r\n      Telefono2: null,\r\n      Email: null,\r\n      dialog: false,\r\n      cedula: \"\",\r\n      telefono: 0,\r\n      Expedientes: []\r\n    };\r\n  },\r\n  methods: {\r\n    created() {\r\n      db.collection(\"Expedientes1\")\r\n        .where(\"NumeroIdentificacion\", \"==\", this.cedula)\r\n        .onSnapshot(res => {\r\n          const changes = res.docChanges();\r\n          changes.forEach(change => {\r\n            if (change.type === \"added\") {\r\n              this.Expedientes.push({\r\n                ...change.doc.data(),\r\n                id: change.doc.id\r\n              });\r\n            }\r\n          });\r\n        });\r\n    },\r\n    // load() {\r\n    //   db.collection(\"Expedientes1\")\r\n    //     .where(\"PrimerNombre\", \"==\", this.nombre1)\r\n    //     .get()\r\n    //     .then(function(querySnapshot) {\r\n    //       querySnapshot.forEach(function(doc) {\r\n    //         // doc.data() is never undefined for query doc snapshots\r\n    //         console.log(doc.id, \" => \", doc.data());\r\n    //       });\r\n    //     })\r\n    //     .catch(function(error) {\r\n    //       console.log(\"Error getting documents: \", error);\r\n    //     });\r\n    // },\r\n    deleted(Expediente) {\r\n      db.collection(\"Expedientes1\")\r\n        .doc(Expediente.id)\r\n        .delete()\r\n        .then(function() {\r\n          console.log(\"Document successfully deleted!\");\r\n        })\r\n        .catch(function(error) {\r\n          console.error(\"Error removing document: \", error);\r\n        });\r\n    },\r\n    update(Expediente) {\r\n      const actualizacion = db.collection(\"Expedientes1\").doc(Expediente.id);\r\n      return actualizacion\r\n        .update({\r\n          PrimerNombre: this.PrimerNombre,\r\n          SegundoNombre: this.SegundoNombre,\r\n          PrimerApellido: this.PrimerApellido,\r\n          SegundoApellido: this.SegundoApellido,\r\n          IDTipoIdentificacion: this.IDTipoIdentificacion,\r\n          NumeroIdentificacion: this.NumeroIdentificacion,\r\n          Genero: this.Genero,\r\n          FechaNacimiento: this.FechaNacimiento,\r\n          Edad: this.Edad,\r\n          Telefono1CodigoPais: this.Telefono1CodigoPais,\r\n          Telefono2CodigoPais: this.Telefono2CodigoPais,\r\n          Telefono1: this.Telefono1,\r\n          Telefono2: this.Telefono2,\r\n          Email: this.Email\r\n        })\r\n        .then(function() {\r\n          console.log(\"Document successfully updated!\");\r\n        })\r\n        .catch(function(error) {\r\n          // The document probably doesn't exist.\r\n          console.error(\"Error updating document: \", error);\r\n        });\r\n    }\r\n  }\r\n};\r\n</script>\r\n"]}]}