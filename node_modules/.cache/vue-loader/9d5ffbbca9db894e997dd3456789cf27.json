 class=\"text-center \"\r\n          v-for=\"Expediente in Expedientes\"\r\n          :key=\"Expediente.IDExpediente\"\r\n        >\r\n          <div>\r\n            <v-icon>person</v-icon>\r\n            <br />\r\n            {{ Expediente.PrimerNombre }}\r\n            {{ Expediente.SegundoNombre }}\r\n            {{ Expediente.PrimerApellido }}\r\n            {{ Expediente.SegundoApellido }}\r\n          </div>\r\n          <div>\r\n            <v-icon>credit_card</v-icon>\r\n            <br />Cédula:\r\n            {{ Expediente.NumeroIdentificacion }}\r\n          </div>\r\n          <div>\r\n            <v-icon>phone</v-icon>\r\n            <br />Teléfono: {{ Expediente.Telefono1 }}\r\n          </div>\r\n          <div>\r\n            <v-icon>mail</v-icon><br />\r\n            Email: {{ Expediente.Email }}\r\n          </div>\r\n        </div>\r\n      </v-card>\r\n    </v-card>\r\n  </v-dialog>\r\n</template>\r\n\r\n<script>\r\nimport { db } from \"@/main\";\r\nexport default {\r\n  datas() {\r\n    return {\r\n      dialog: false\r\n    };\r\n  },\r\n  data() {\r\n    return {\r\n      Expedientes: []\r\n    };\r\n  },\r\n  methods: {\r\n    created() {\r\n      db.collection(\"Expedientes\")\r\n        .where(\"PrimerNombre\", \"==\", this.nombre1)\r\n        .where(\"PrimerApellido\", \"==\", this.apellido1)\r\n        .onSnapshot(res => {\r\n          const changes = res.docChanges();\r\n\r\n          changes.forEach(change => {\r\n            if (change.type === \"added\") {\r\n              this.Expedientes.push({\r\n                ...change.doc.data(),\r\n                id: change.doc.id\r\n              });\r\n            }\r\n          });\r\n        });\r\n    }\r\n  }\r\n};\r\n</script>\r\n"]}]}