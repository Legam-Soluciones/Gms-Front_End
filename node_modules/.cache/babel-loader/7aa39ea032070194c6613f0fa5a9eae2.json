AAA,KAAA,cAPA;AAQA,QAAA,IAAA,EAAA,MAAA,CAAA,QAAA,CAAA,KAAA,IAAA,CAAA,EAAA,aAAA;AARA,OAAA;AAUA,MAAA,EAAA,CAAA,UAAA,CAAA,QAAA,EACA,GADA,CACA,MADA,EAEA,IAFA,CAEA,YAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,4BAAA;AACA,OAJA;AAKA;AAlBA,GAhBA;AAoCA,EAAA,QAAA,EAAA;AACA,IAAA,aADA,2BACA;AACA,aAAA,KAAA,IAAA,GAAA,MAAA,CAAA,QAAA,CAAA,KAAA,IAAA,CAAA,EAAA,aAAA,CAAA,GAAA,EAAA;AACA;AAHA;AApCA,CAAA","sourcesContent":["<template>\r\n  <v-row justify=\"center\">\r\n    <v-dialog v-model=\"dialog\" persistent max-width=\"600px\">\r\n      <template v-slot:activator=\"{ on, attrs }\">\r\n        <v-btn color=\"teal\" tile outlined v-bind=\"attrs\" v-on=\"on\">\r\n          Agregar Cita a la Agenda\r\n        </v-btn>\r\n      </template>\r\n      <v-card ref=\"form\">\r\n        <v-card-title>\r\n          <span class=\"headline\"> Agregar Nueva Cita </span>\r\n        </v-card-title>\r\n        <v-card-text>\r\n          <v-container>\r\n            <v-row>\r\n              <v-col cols=\"12\" sm=\"6\" md=\"4\">\r\n                <v-text-field\r\n                  label=\"Nombre Paciente\"\r\n                  v-model=\"nombre\"\r\n                  required\r\n                ></v-text-field>\r\n              </v-col>\r\n              <v-col cols=\"12\" sm=\"6\" md=\"4\">\r\n                <v-text-field\r\n                  label=\" Teléfono \"\r\n                  v-model=\"telefono\"\r\n                ></v-text-field>\r\n              </v-col>\r\n              <v-col cols=\"12\" sm=\"6\" md=\"4\">\r\n                <v-text-field\r\n                  label=\"Doctor/a:\"\r\n                  hint=\"Persona que realizará el procedimiento\"\r\n                  persistent-hint\r\n                  required\r\n                  v-model=\"doctor\"\r\n                ></v-text-field>\r\n              </v-col>\r\n              <v-col cols=\"12\">\r\n                <v-text-field\r\n                  label=\"Correo Eléctronico\"\r\n                  v-model=\"email\"\r\n                  required\r\n                ></v-text-field>\r\n              </v-col>\r\n              <v-col cols=\"12\">\r\n                <v-text-field\r\n                  label=\" Confirmada / No Confirmada \"\r\n                  v-model=\"confirmacion\"\r\n                  required\r\n                ></v-text-field>\r\n              </v-col>\r\n              <v-col cols=\"12\" sm=\"6\">\r\n                <v-select\r\n                  :items=\"['0-17', '18-29', '30-54', '54+']\"\r\n                  label=\"Edad de Paciente*\"\r\n                  required\r\n                  v-model=\"edad\"\r\n                ></v-select>\r\n              </v-col>\r\n              <v-col cols=\"12\" sm=\"6\">\r\n                <v-autocomplete\r\n                  :items=\"[\r\n                    'Dermapen',\r\n                    'Carboxterapia',\r\n                    'Limpieza Facial',\r\n                    'Masaje',\r\n                    'Desintoxicación Iónica',\r\n                    'Termo Gimnasia',\r\n                    'Implanon',\r\n                    'Masaje Cervical',\r\n                    'PRP',\r\n                    'Relleno de Labios',\r\n                    'Eliminación de Ronquidos',\r\n                    'Botox',\r\n                    'Ácido hialurónico',\r\n                    'Eliminación de estrias'\r\n                  ]\"\r\n                  label=\"Procedimientos a afectuar\"\r\n                  multiple\r\n                  v-model=\"procedimientos\"\r\n                ></v-autocomplete>\r\n              </v-col>\r\n              <v-col>\r\n                <calendario />\r\n              </v-col>\r\n            </v-row>\r\n          </v-container>\r\n          <small>* Por Favor llenar todos los espacios *</small>\r\n        </v-card-text>\r\n        <v-card-actions>\r\n          <v-spacer></v-spacer>\r\n          <v-btn color=\"blue darken-1\">Cerrar</v-btn>\r\n          <!-- @click=\"dialog = false\" -->\r\n          <v-btn color=\"blue darken-1\" text @click=\"submit\">Agregar</v-btn>\r\n        </v-card-actions>\r\n      </v-card>\r\n    </v-dialog>\r\n  </v-row>\r\n</template>\r\n\r\n<script>\r\nimport db from \"@/fb\";\r\nimport calendario from \"@/components/Categorias/Agenda/calendario.vue\";\r\nimport format from \"date-fns/format\";\r\nimport parseISO from \"date-fns/parseISO\";\r\n\r\nexport default {\r\n  components: {\r\n    calendario\r\n  },\r\n  data() {\r\n    return {\r\n      nombre: \"\",\r\n      telefono: \"\",\r\n      doctor: \"\",\r\n      email: \"\",\r\n      confirmacion: \"\",\r\n      edad: \"\",\r\n      procedimientos: \"\",\r\n      date: new Date().toISOString().substr(0, 10)\r\n    };\r\n  },\r\n  methods: {\r\n    submit() {\r\n      //  console.log(this.nombre, this.telefono, this.doctor, this.email, this.confirmacion, this.edad, this.procedimientos)\r\n      const agenda = {\r\n        nombre: this.nombre,\r\n        telefono: this.telefono,\r\n        doctor: this.doctor,\r\n        email: this.email,\r\n        confirmacion: this.confirmacion,\r\n        edad: this.edad,\r\n        procedimientos: this.procedimientos,\r\n        date: format(parseISO(this.date), \"yyyy MMM do\")\r\n      };\r\n      db.collection(\"agenda\")\r\n        .add(agenda)\r\n        .then(() => {\r\n          console.log(\"Añadido a la base de datos\");\r\n        });\r\n    }\r\n  },\r\n  computed: {\r\n    formattedDate() {\r\n      return this.date ? format(parseISO(this.date), \"yyyy MMM do\") : \"\";\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"sourceRoot":"src/components/Categorias/Agenda"}]}