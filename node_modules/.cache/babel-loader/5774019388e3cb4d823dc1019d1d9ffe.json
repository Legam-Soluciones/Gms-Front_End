>Agregar Tarea</v-btn\r\n            >\r\n          </v-row>\r\n          <v-dialog v-model=\"dialog\" max-width=\"500\">\r\n            <v-card>\r\n              <v-container>\r\n                <v-form>\r\n                  <v-text-field\r\n                    v-model=\"nuevaTarea\"\r\n                    type=\"text\"\r\n                    label=\"Título de la Tarea\"\r\n                  ></v-text-field>\r\n                  <v-text-field\r\n                    v-model=\"responsables\"\r\n                    type=\"text\"\r\n                    label=\"Responsables\"\r\n                  ></v-text-field>\r\n                  <v-select\r\n                    :items=\"['Correo', 'Llamada', 'Pendiente']\"\r\n                    label=\"Tipo\"\r\n                    v-model=\"tipo\"\r\n                    type=\"text\"\r\n                  ></v-select>\r\n                  <v-text-field\r\n                    v-model=\"notas\"\r\n                    type=\"text\"\r\n                    label=\"Notas\"\r\n                  ></v-text-field>\r\n                  <v-text-field\r\n                    v-model=\"vencimiento\"\r\n                    type=\"date\"\r\n                    label=\"Fecha de vencimiento\"\r\n                  ></v-text-field>\r\n                  <v-btn\r\n                    type=\"submit\"\r\n                    color=\"primay\"\r\n                    class=\"mr-4\"\r\n                    @click.stop=\"dialog = false\"\r\n                    @click=\"añadirTarea()\"\r\n                    >Crear Cita</v-btn\r\n                  >\r\n                </v-form>\r\n              </v-container>\r\n            </v-card>\r\n          </v-dialog>\r\n          <v-container>\r\n            <v-row>\r\n              <v-col>\r\n                <v-toolbar-title> Tareas por hacer </v-toolbar-title>\r\n                <v-card\r\n                  class=\"pa-3 ma-2 text center\"\r\n                  max-width=\"300\"\r\n                  max-height=\"400\"\r\n                  v-for=\"Tarea in Tareas\"\r\n                  :key=\"Tarea.id\"\r\n                >\r\n                  <div v-if=\"Tarea.estado == true\">\r\n                    <div v-if=\"Tarea.edit == false\">\r\n                      <v-row>\r\n                        <v-list-item-title\r\n                          ><p class=\"text-center\">\r\n                            {{ Tarea.Nombre }} <br />Responsables:<br />{{\r\n                              Tarea.Responsables\r\n                            }}<br />\r\n                            Tipo:<br />{{ Tarea.Tipo }}<br />\r\n                            Notas:<br />{{ Tarea.Notas }}<br />\r\n                            Fecha de Vencimiento:<br />{{\r\n                              Tarea.FechadeVencimiento\r\n                            }}<br />\r\n                          </p>\r\n                          <div class=\"text-center\">\r\n                            <v-btn icon color=\"red\" @click=\"borrarTarea(Tarea)\">\r\n                              <v-icon>delete</v-icon>\r\n                            </v-btn>\r\n                            <v-btn\r\n                              icon\r\n                              color=\"blue\"\r\n                              @click=\"editarTarea(Tarea), (dialog2 = true)\"\r\n                            >\r\n                              <v-icon>add_comment</v-icon>\r\n                            </v-btn>\r\n                            <v-btn\r\n                              icon\r\n                              color=\"green\"\r\n                              @click=\"updateEstadoTarea(Tarea)\"\r\n                            >\r\n                              <v-icon>check_circle</v-icon>\r\n                            </v-btn>\r\n                          </div>\r\n                        </v-list-item-title>\r\n                      </v-row>\r\n                    </div>\r\n                  </div>\r\n                  <div v-if=\"Tarea.edit == true\">\r\n                    <v-dialog persistent v-model=\"dialog2\" max-width=\"500\">\r\n                      <v-card>\r\n                        <v-container>\r\n                          <v-form>\r\n                            <v-text-field\r\n                              v-model=\"Tarea.Nombre\"\r\n                              type=\"text\"\r\n                              label=\"Título de la Tarea\"\r\n                            ></v-text-field>\r\n                            <v-text-field\r\n                              v-model=\"Tarea.Responsables\"\r\n                              type=\"text\"\r\n                              label=\"Responsables\"\r\n                            ></v-text-field>\r\n                            <v-select\r\n                              :items=\"['Correo', 'Llamada', 'Pendiente']\"\r\n                              label=\"Tipo\"\r\n                              v-model=\"Tarea.Tipo\"\r\n                              type=\"text\"\r\n                            ></v-select>\r\n                            <v-text-field\r\n                              v-model=\"Tarea.Notas\"\r\n                              type=\"text\"\r\n                              label=\"Notas\"\r\n                            ></v-text-field>\r\n                            <v-text-field\r\n                              v-model=\"Tarea.FechadeVencimiento\"\r\n                              type=\"date\"\r\n                              label=\"Fecha de vencimiento\"\r\n                            ></v-text-field>\r\n                            <v-btn\r\n                              type=\"submit\"\r\n                              color=\"primay\"\r\n                              class=\"mr-4\"\r\n                              @click.stop=\"dialog2 = false\"\r\n                              @click=\"update(Tarea)\"\r\n                              >Guardar</v-btn\r\n                            >\r\n                            <v-btn\r\n                              color=\"secondary\"\r\n                              @click.stop=\"dialog2 = false\"\r\n                              @click=\"cancelar(Tarea)\"\r\n                              >Cancelar</v-btn\r\n                            >\r\n                          </v-form>\r\n                        </v-container>\r\n                      </v-card>\r\n                    </v-dialog>\r\n                  </div>\r\n                </v-card>\r\n              </v-col>\r\n              <v-col>\r\n                <v-toolbar-title> Tareas completadas </v-toolbar-title>\r\n                <v-card\r\n                  class=\"pa-3 ma-2 text center\"\r\n                  max-width=\"250\"\r\n                  v-for=\"Tarea in Tareas\"\r\n                  :key=\"Tarea.id\"\r\n                >\r\n                  <div v-if=\"Tarea.estado == false\">\r\n                    <v-row>\r\n                      <v-list-item-title>\r\n                        <p\r\n                          class=\"black--text text-center text-decoration-line-through\"\r\n                        >\r\n                          {{ Tarea.Nombre }} <br />\r\n                          Responsables:<br />{{ Tarea.Responsables }}<br />\r\n                          Notas:<br />{{ Tarea.Notas }}<br />\r\n                          Fecha de Vencimiento:<br />{{\r\n                            Tarea.FechadeVencimiento\r\n                          }}<br />\r\n                        </p>\r\n                        <div class=\"text-center\">\r\n                          <v-btn icon color=\"red\" @click=\"borrarTarea(Tarea)\">\r\n                            <v-icon>delete</v-icon>\r\n                          </v-btn>\r\n                          <v-btn\r\n                            icon\r\n                            color=\"red\"\r\n                            @click=\"updateEstadoTarea(Tarea)\"\r\n                          >\r\n                            <v-icon>remove_circle</v-icon>\r\n                          </v-btn>\r\n                        </div>\r\n                      </v-list-item-title>\r\n                    </v-row>\r\n                  </div>\r\n                </v-card>\r\n              </v-col>\r\n            </v-row></v-container\r\n          >\r\n        </template>\r\n        <template v-slot:footer>\r\n          <v-toolbar class=\"mt-2\" src=\"@/assets/img12.jpg\" dark flat>\r\n            <v-toolbar-title class=\"subheading\">\r\n              - Más Salud Slim Clinic -\r\n            </v-toolbar-title>\r\n          </v-toolbar>\r\n        </template>\r\n      </v-container>\r\n    </template>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nimport { db } from \"@/main\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      Tareas: [],\r\n      nuevaTarea: \"\",\r\n      responsables: \"\",\r\n      tipo: \"\",\r\n      notas: \"\",\r\n      vencimiento: null,\r\n      edit: false,\r\n      estado: true,\r\n      dialog: false,\r\n      dialog2: false\r\n    };\r\n  },\r\n  created() {\r\n    this.getTareas();\r\n  },\r\n  methods: {\r\n    getTareas() {\r\n      const Tareas = [];\r\n      db.collection(\"Tareas\").onSnapshot(res => {\r\n        const changes = res.docChanges();\r\n        changes.forEach(change => {\r\n          if (change.type === \"added\") {\r\n            Tareas.push({\r\n              ...change.doc.data(),\r\n              id: change.doc.id\r\n            });\r\n            this.Tareas = Tareas;\r\n          }\r\n        });\r\n      });\r\n    },\r\n\r\n    async añadirTarea() {\r\n      try {\r\n        if (this.nuevaTarea != null) {\r\n          db.collection(\"Tareas\").add({\r\n            Nombre: this.nuevaTarea,\r\n            Responsables: this.responsables,\r\n            Tipo: this.tipo,\r\n            Notas: this.notas,\r\n            FechadeVencimiento: this.vencimiento,\r\n            edit: false,\r\n            estado: true\r\n          });\r\n          this.getTareas();\r\n          this.nuevaTarea = \"\";\r\n          this.responsables = \"\";\r\n          this.tipo = \"\";\r\n          this.notas = \"\";\r\n          this.vencimiento = \"\";\r\n        } else {\r\n          alert(\"Error\");\r\n        }\r\n      } catch (error) {\r\n        console.log(error);\r\n      }\r\n    },\r\n    async editarTarea(Tarea) {\r\n      try {\r\n        if (this.edit == false) {\r\n          db.collection(\"Tareas\")\r\n            .doc(Tarea.id)\r\n            .update({\r\n              edit: true\r\n            });\r\n          this.getTareas();\r\n        } else {\r\n          alert(\"Error\");\r\n        }\r\n      } catch (error) {\r\n        console.log(error);\r\n      }\r\n    },\r\n\r\n    async cancelar(Tarea) {\r\n      try {\r\n        if (Tarea.edit == true) {\r\n          db.collection(\"Tareas\")\r\n            .doc(Tarea.id)\r\n            .update({\r\n              edit: false\r\n            });\r\n          this.getTareas();\r\n        } else {\r\n          alert(\"Error\");\r\n        }\r\n      } catch (error) {\r\n        console.log(error);\r\n      }\r\n    },\r\n    updateEstadoTarea(Tarea) {\r\n      try {\r\n        if (Tarea.estado != null) {\r\n          db.collection(\"Tareas\")\r\n            .doc(Tarea.id)\r\n            .update({\r\n              estado: !Tarea.estado\r\n            });\r\n          this.getTareas();\r\n        } else {\r\n          alert(\"Error\");\r\n        }\r\n      } catch (error) {\r\n        console.log(error);\r\n      }\r\n    },\r\n\r\n    async update(Tarea) {\r\n      try {\r\n        if (Tarea.edit == true) {\r\n          db.collection(\"Tareas\")\r\n            .doc(Tarea.id)\r\n            .update({\r\n              Nombre: Tarea.Nombre,\r\n              Responsables: Tarea.Responsables,\r\n              Tipo: Tarea.Tipo,\r\n              Notas: Tarea.Notas,\r\n              FechadeVencimiento: Tarea.FechadeVencimiento,\r\n              edit: false\r\n            });\r\n          this.getTareas();\r\n        } else {\r\n          alert(\"Error\");\r\n        }\r\n      } catch (error) {\r\n        console.log(error);\r\n      }\r\n    },\r\n    async borrarTarea(Tarea) {\r\n      try {\r\n        if (Tarea.edit == false) {\r\n          db.collection(\"Tareas\")\r\n            .doc(Tarea.id)\r\n            .delete();\r\n          this.getTareas();\r\n        } else {\r\n          alert(\"Error\");\r\n        }\r\n      } catch (error) {\r\n        console.log(error);\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"sourceRoot":"src/components/Categorias/Tareas"}]}