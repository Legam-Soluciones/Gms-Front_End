           </div>\r\n            <div v-if=\"registro == 2\">\r\n              <v-text-field \r\n              v-model=\"Ubicacion\" \r\n              type=\"text\" \r\n              label=\"Lugar\">\r\n              </v-text-field>\r\n              <v-text-field\r\n                v-model=\"Nombre\"\r\n                type=\"text\"\r\n                label=\"Nombre completo del paciente\"\r\n              >\r\n              </v-text-field>\r\n              <v-text-field\r\n                v-model=\"Identificacion\"\r\n                type=\"text\"\r\n                label=\"Numero de Identificación\"\r\n              >\r\n              </v-text-field>\r\n              <v-text-field\r\n                v-model=\"Procedimiento\"\r\n                type=\"text\"\r\n                label=\"Procedimiento a realizar\"\r\n              >\r\n              </v-text-field>\r\n              <v-text-field\r\n                v-model=\"Profesional\"\r\n                type=\"text\"\r\n                label=\"Nombre completo del profesional\"\r\n              >\r\n              </v-text-field>\r\n              <v-text-field\r\n                v-model=\"CodigoProfesional\"\r\n                type=\"text\"\r\n                label=\"Codigo del profesional\"\r\n              >\r\n              </v-text-field>\r\n            </div>\r\n            <div v-if=\"registro == 3\">\r\n              <v-text-field \r\n              v-model=\"Ubicacion\"\r\n              type=\"text\" \r\n              label=\"Lugar\">\r\n              </v-text-field>\r\n              <v-text-field\r\n                v-model=\"Nombre\"\r\n                type=\"text\"\r\n                label=\"Nombre completo del paciente\"\r\n              >\r\n              </v-text-field>\r\n              <v-text-field\r\n                v-model=\"Procedimiento\"\r\n                type=\"text\"\r\n                label=\"Procedimiento a realizar\"\r\n              >\r\n              </v-text-field>\r\n              <v-text-field\r\n                v-model=\"Profesional\"\r\n                type=\"text\"\r\n                label=\"Nombre completo del profesional\"\r\n              >\r\n              </v-text-field>\r\n            </div>\r\n            <div v-if=\"registro == 4\">\r\n              <v-text-field \r\n              v-model=\"Ubicacion\" \r\n              type=\"text\" \r\n              label=\"Lugar\">\r\n              </v-text-field>\r\n              <v-text-field\r\n                v-model=\"Nombre\"\r\n                type=\"text\"\r\n                label=\"Nombre completo del paciente\"\r\n              >\r\n              </v-text-field>\r\n              <v-text-field\r\n                v-model=\"Identificacion\"\r\n                type=\"text\"\r\n                label=\"Numero de Identificación\"\r\n              >\r\n              </v-text-field>\r\n              <v-text-field\r\n                v-model=\"Profesional\"\r\n                type=\"text\"\r\n                label=\"Nombre completo del profesional\"\r\n              >\r\n              </v-text-field>\r\n              <v-text-field\r\n                v-model=\"CodigoProfesional\"\r\n                type=\"text\"\r\n                label=\"Codigo del profesional\"\r\n              >\r\n              </v-text-field>\r\n              <v-text-field\r\n                v-model=\"HEntrada\"\r\n                type=\"text\"\r\n                label=\"Hora de Entrada\"\r\n              >\r\n              </v-text-field>\r\n              <v-text-field\r\n                v-model=\"HSalida\"\r\n                type=\"text\"\r\n                label=\"Hora de Salida\"\r\n              >\r\n              </v-text-field>\r\n            </div>\r\n            <div v-if=\"registro == 6\">\r\n              <v-text-field \r\n              v-model=\"Autovocher\" \r\n              type=\"text\" \r\n              label=\"Autovocher\">\r\n              </v-text-field>\r\n              <v-text-field\r\n                v-model=\"Datafonoutilizado\"\r\n                type=\"text\"\r\n                label=\"Datáfono utilizado\"\r\n              >\r\n              </v-text-field>\r\n              <v-text-field \r\n              v-model=\"Impuesto\" \r\n              type=\"text\" \r\n              label=\"IVA\">\r\n              </v-text-field>\r\n              <v-text-field v-model=\"Moneda\" \r\n              type=\"text\" \r\n              label=\"Moneda\">\r\n              </v-text-field>\r\n              <v-text-field \r\n              v-model=\"Recibido\" \r\n              type=\"text\" \r\n              label=\"Recibido\">\r\n              </v-text-field>\r\n              <v-text-field \r\n              v-model=\"Voucher\" \r\n              type=\"text\" \r\n              label=\"Voucher\">\r\n              </v-text-field>\r\n              <v-text-field \r\n              v-model=\"Datafono\" \r\n              type=\"text\" \r\n              label=\"Datáfono\">\r\n              </v-text-field>\r\n            </div>\r\n            <v-btn @click=\"addData()\">\r\n              Llenar Datos\r\n            </v-btn>\r\n            <v-btn @click=\"addDocumento()\">\r\n              Guardar Datos\r\n            </v-btn>\r\n            <v-btn @click=\"created()\">\r\n              Ver datos\r\n            </v-btn>\r\n          </v-card-text>\r\n        </v-card>\r\n      </v-hover>\r\n    </v-col>\r\n\r\n    <v-col cols=\"12\" sm=\"6\">\r\n      <v-hover v-slot:default=\"{ hover }\" close-delay=\"200\">\r\n        <v-card\r\n          :elevation=\"hover ? 16 : 2\"\r\n          class=\"mx-auto\"\r\n          max-height=\"2000px\"\r\n          max-width=\"1200\"\r\n          v-for=\"Documento in Documentos\"\r\n          :key=\"Documento.id\"\r\n        >\r\n          <div v-if=\"elegirdocumento == 1\">\r\n            <div class=\"d-flex justify-center\">\r\n              <v-img\r\n                src=\"@/assets/LogoM.png\"\r\n                max-height=\"400px\"\r\n                max-width=\"300px\"\r\n                justify-center\r\n              ></v-img>\r\n            </div>\r\n            <p class=\"text-center\">\r\n              Consentimiento Informado <br />\r\n              Fecha: {{ Documento.Fecha }} <br />\r\n            </p>\r\n            Lugar: {{ Documento.Lugar }} <br />\r\n            <br />El suscrito (a) {{ Documento.Suscrito }} en mi condición de\r\n            usuario, padre o madre, tutor, apoderado legal de:\r\n            {{ Documento.NombreCompleto }}, doy mi consentimiento informado para\r\n            que: Se practique por parte del personal autorizado del centro de\r\n            salud, el procedimiento o programa que a continuación se detalla:\r\n            {{ Documento.Procedimiento }} <br /><br />\r\n\r\n            Por tanto hago constar que de previo a la firma de este\r\n            consentimiento informado, se han cumplido las siguientes\r\n            condiciones: <br />\r\n            <br />\r\n            1. Recibí información sencilla y completa por parte de\r\n            {{ Documento.Profesional }}, quien manifiesta ser funcionario\r\n            calificado del centro de salud. En esta, se me proporciono\r\n            información suficiente y comprensible sobre los beneficios y riesgos\r\n            relacionados con las actividades consentidas, los cuales son: <br />\r\n            <br />\r\n\r\n            Riesgos: {{ Documento.Riesgos }} <br />\r\n            <br />\r\n            Beneficios: {{ Documento.Beneficios }}<br />\r\n            <br />\r\n            2. Se me ha leído y explicado en forma completa el contenido de este\r\n            documento, aclarando a la vez, todas mis dudas a satisfacción.\r\n            <br />\r\n            <br />\r\n            Aclaro, eso si, que durante mi atención podre cambiar de opinión y\r\n            cancelar la presente autorización, de lo cual se dejara constancia\r\n            escrita en el expediente, sin que ello signifique que se me negara\r\n            los restantes servicios de salud. <br /><br />\r\n            <Firma />\r\n          </div>\r\n\r\n\r\n          <div v-if=\"elegirdocumento == 2\">\r\n            <div class=\"d-flex justify-center\">\r\n              <v-img\r\n                src=\"@/assets/LogoM.png\"\r\n                max-height=\"400px\"\r\n                max-width=\"300px\"\r\n                justify-center\r\n              ></v-img>\r\n            </div>\r\n            <p class=\"text-center\">\r\n              Consentimiento Informado <br />\r\n              Fecha: {{ Documento.Fecha }} <br />\r\n            </p>\r\n            Lugar: {{ Documento.Lugar }} <br />\r\n            <br />{{ Documento.NombreCompleto }} cédula {{Documento.Identificacion}}, \r\n            he sido valorado por el Dr.: {{ Documento.Profesional }}, código profesional \r\n            {{Documento.CodigoProfesional}} quien es mi médico \r\n            tratante y me ha informado acerca de mi enfermedad y/o padecimiento y el \r\n            tratamiento de la misma. <br /> <br />\r\n            He sido informado sobre el procedimiento a realizar, los riesgos y beneficios que \r\n            voy a obtener, así como las indicaciones postoperatorias a seguir, me he enterado de las\r\n            consecuencias en caso de no recibir el tratamiento. <br /> <br />\r\n            He realizado las preguntas necesarias y el médico me ha dado respuestas comprensibles,\r\n            por lo tanto doy mi consentimiento para: {{Documento.Procedimiento}}\r\n            <Firma />\r\n          </div>\r\n\r\n             <div v-if=\"elegirdocumento == 3\">\r\n            <div class=\"d-flex justify-center\">\r\n              <v-img\r\n                src=\"@/assets/LogoM.png\"\r\n                max-height=\"400px\"\r\n                max-width=\"300px\"\r\n                justify-center\r\n              ></v-img>\r\n            </div>\r\n            <p class=\"text-center\">\r\n              Consentimiento Informado <br />\r\n              Fecha: {{ Documento.Fecha }} <br />\r\n            </p>\r\n            Lugar: {{ Documento.Lugar }} <br />\r\n            <br />El suscrito (a) {{ Documento.NombreCompleto }}, en mi condición de usuario, doy mi\r\n            consentimiento informado para que se practique por parte del personal autorizado del centro de salud,\r\n            el procedimiento de mesoterapia. <br /> <br />\r\n\r\n            Por tanto hago constar que de previo a la firma de este consentimiento informado, se han cumplido\r\n            las siguientes condiciones: <br /> <br />\r\n\r\n            1. Recibí información sencilla y completa por parte de un funcionario calificado del centro de salud.\r\n            En esta, se me proporciono información suficiente y comprensible sobre los beneficios y riesgos \r\n            relacionados con las actividades consentidas, los cuales son: <br /> <br />\r\n\r\n            Riesgos: edema, snagrado, hematomas varios en la zona de aplicación. <br /> <br />\r\n\r\n            Beneficios: disminución de mm a cm en el área de la aplicación, disminución de piel naranja y leve tonificación. <br /> <br />\r\n\r\n            2. Se me ha leído y explicado en forma completa el contenido de este documento, aclarando a la vez,\r\n            todas mis dudas a satisfacción. <br /> <br />\r\n            Aclaro, eso si, que durante mi atención podré cambiar de opinión y cancelar la presente autorización, de lo cual\r\n            se dejara constancia escrita en el expediente, sin que ello signifique que se me negara los restantes servicios de salud.\r\n            <Firma />\r\n          </div>\r\n\r\n          <div v-if=\"elegirdocumento == 4\">\r\n            <div class=\"d-flex justify-center\">\r\n              <v-img\r\n                src=\"@/assets/LogoM.png\"\r\n                max-height=\"400px\"\r\n                max-width=\"300px\"\r\n                justify-center\r\n              ></v-img>\r\n            </div>\r\n            <p class=\"text-center\">\r\n              Justificante Médico <br />\r\n              Fecha: {{ Documento.Fecha }} <br />\r\n            </p>\r\n            Dr (a): {{ Documento.Profesional }} <br /> <br />\r\n            Código: {{ Documento.CodigoProfesional }} <br /> <br />\r\n            <br /> La presente es para saludarle, y a la vez comunicarle que el día de hoy el (la) señor (a) \r\n            {{Documento.NombreCompleto}} con cédula de identidad número {{Documento.Identificacion}} se presentó \r\n            a la consulta médica a las {{Documento.HoradeEntrada}}  hrs y se retiro a las {{Documento.HoradeSalida}} hrs.\r\n            <br /> <br />\r\n            El paciente fue dado de alta, sin embargo, se le recomienda guardar reposo por el resto del día. <br /><br />\r\n            Sírvase el interesado hacer uso del presente para los fines que más le convengan. Agradeciendo la atención\r\n            brindada y ayuda brindada a la paciente, se despide. <br /><br />\r\n            Atentamente:\r\n            <Firma />\r\n          </div>\r\n\r\n          <div v-if=\"elegirdocumento == 5\">\r\n            <div class=\"d-flex justify-center\">\r\n              <v-img\r\n                src=\"@/assets/LogoM.png\"\r\n                max-height=\"400px\"\r\n                max-width=\"300px\"\r\n                justify-center\r\n              ></v-img>\r\n            </div>\r\n            <p class=\"text-center\">\r\n              Consentimiento informado PRP capilar <br />\r\n              Fecha: {{ Documento.Fecha }} <br />\r\n            </p>\r\n            Lugar: {{Documento.Lugar}} <br /> \r\n            <br /> El suscrito (a) {{Documento.Nombre}}, en mi condición de usuario, doy mi consentimiento \r\n            informado para que se practique por parte del personal autoirzado del centro de salud, el procedimiento de\r\n            Plasma rico en plaquetas capilar.\r\n            <br /> <br />\r\n            Por tanto hago constar que de previo a la firma de este consentimiento informado, se han cumplido las siguentes\r\n            condiciones:\r\n             <br /><br />\r\n             1. Recibí información sencilla y completa por parte de un funcionario calificado del centro de salud. En esta,\r\n             se me proporciono información suficiente y comprensible sobre los beneficios y riesgos relacionados con las \r\n             actividades consentidas, los cuales son: <br /><br />\r\n             Riesgos: edema, sangrado, dolor. <br /><br />\r\n             Beneficios: disminución de la caída del cabello, engrosamiento del folículo piloso, aceleración de la\r\n             fase anágena del cabello. <br /><br />\r\n             2. Se me ha leído y explicado en forma completa el contenido de este documento, aclarando a la vez,\r\n             todas mis dudas a satisfacción. <br /> <br />\r\n             Aclaro, eso si, que durante mi atención podré cambiar de opinión y cancelar la presente autorización, \r\n             de lo cuál se dejara constancia escrita en el expediente, sin que ello signifique que se me negara los \r\n             restantes servicios de salud.\r\n            <Firma />\r\n          </div>\r\n\r\n          <div v-if=\"elegirdocumento == 6\">\r\n            <div class=\"d-flex justify-center\">\r\n              <v-img\r\n                src=\"@/assets/LogoM.png\"\r\n                max-height=\"400px\"\r\n                max-width=\"300px\"\r\n                justify-center\r\n              ></v-img>\r\n            </div>\r\n            <br />\r\n            Fecha: {{ Documento.Fecha }} <br /> \r\n            Cliente: {{Documento.Nombre}} <br />\r\n            Cédula: {{Documento.Identificacion}} <br />\r\n            Auto Voucher: {{Documento.Autovocher}} <br />\r\n            Datáfono utilizado: {{Documento.Datafonoutilizado}} <br />\r\n            Monto IVA: {{Documento.Impuesto}} <br />\r\n            Moneda: {{ Documento.Moneda }} <br />\r\n            <br /> \r\n            Yo, {{Documento.NombreCompleto}}, portador de la cédula de identidad número {{Documento.Identificacion}}, hago\r\n            constar que he recibido la suma de {{Documento.Recibido}} por concepto de reintrego del IVA según lo indica la ley,\r\n            esto por cancelar mi servicio médico con tarjeta de crédito u débido, la autorización de mi vaucher es la número \r\n            {{Documento.Voucher}} y este fue incluido en el cierre de datáfono del {{Documento.Datafono}} de este mismo día.\r\n            <Firma />\r\n          </div>\r\n\r\n         <div v-if=\"elegirdocumento == 7\">\r\n            <div class=\"d-flex justify-center\">\r\n              <v-img\r\n                src=\"@/assets/LogoM.png\"\r\n                max-height=\"400px\"\r\n                max-width=\"300px\"\r\n                justify-center\r\n              ></v-img>\r\n            </div>\r\n            <p class=\"text-center\">\r\n              Consentimiento PRP facial <br />\r\n              Fecha: {{ Documento.Fecha }} <br />\r\n            </p>\r\n            Lugar: {{Documento.Lugar}} <br /> \r\n            <br /> El suscrito (a) {{Documento.Nombre}}, en mi condición de usuario, doy mi consentimiento \r\n            informado para que se practique por parte del personal autoirzado del centro de salud, el procedimiento de\r\n            plasma rico en plaquetas en la zona facial.\r\n            <br /> <br />\r\n            Por tanto hago constar que de previo a la firma de este consentimiento informado, se han cumplido las siguentes\r\n            condiciones:\r\n             <br /><br />\r\n             1. Recibí información sencilla y completa por parte de un funcionario calificado del centro de salud. En esta,\r\n             se me proporciono información suficiente y comprensible sobre los beneficios y riesgos relacionados con las \r\n             actividades consentidas, los cuales son: <br /><br />\r\n             Riesgos: edema, eritema y hematomas varios. <br /><br />\r\n             Beneficios: hidratación, estimulación de las células del rostro para producción de colágeno, tensión facial\r\n             y disminución de líneas de expresión dinámicas. <br /><br />\r\n             2. Se me ha leído y explicado en forma completa el contenido de este documento, aclarando a la vez,\r\n             todas mis dudas a satisfacción. <br /> <br />\r\n             Aclaro, eso si, que durante mi atención podré cambiar de opinión y cancelar la presente autorización, \r\n             de lo cuál se dejara constancia escrita en el expediente, sin que ello signifique que se me negara los \r\n             restantes servicios de salud.\r\n            <Firma />\r\n          </div> \r\n          <div v-if=\"elegirdocumento == 8\">\r\n            <div class=\"d-flex justify-center\">\r\n              <v-img\r\n                src=\"@/assets/LogoM.png\"\r\n                max-height=\"400px\"\r\n                max-width=\"300px\"\r\n                justify-center\r\n              ></v-img>\r\n            </div>\r\n            <p class=\"text-center\">\r\n              Consentimiento informado disminución de estrías <br />\r\n              Fecha: {{ Documento.Fecha }} <br />\r\n            </p>\r\n            Lugar: {{Documento.Lugar}} <br /> \r\n            <br /> El suscrito (a) {{Documento.Nombre}}, en mi condición de usuario, doy mi consentimiento \r\n            informado para que se practique por parte del personal autoirzado del centro de salud, el procedimiento de\r\n            disminución de estrías con plasma pen y dermapen.\r\n            <br /> <br />\r\n            Por tanto hago constar que de previo a la firma de este consentimiento informado, se han cumplido las siguentes\r\n            condiciones:\r\n             <br /><br />\r\n             1. Recibí información sencilla y completa por parte de un funcionario calificado del centro de salud. En esta,\r\n             se me proporciono información suficiente y comprensible sobre los beneficios y riesgos relacionados con las \r\n             actividades consentidas, los cuales son: <br /><br />\r\n             Riesgos: edema, sangrado, hematomas varios en la zona de aplicación. <br /><br />\r\n             Beneficios: disminución del tamñao y tonalidad de las estrías en tratamiento. <br /><br />\r\n             2. Se me ha leído y explicado en forma completa el contenido de este documento, aclarando a la vez,\r\n             todas mis dudas a satisfacción. <br /> <br />\r\n             Aclaro, eso si, que durante mi atención podré cambiar de opinión y cancelar la presente autorización, \r\n             de lo cuál se dejara constancia escrita en el expediente, sin que ello signifique que se me negara los \r\n             restantes servicios de salud.\r\n            <Firma />\r\n          </div> \r\n\r\n          <div v-if=\"elegirdocumento == 9\">\r\n            <div class=\"d-flex justify-center\">\r\n              <v-img\r\n                src=\"@/assets/LogoM.png\"\r\n                max-height=\"400px\"\r\n                max-width=\"300px\"\r\n                justify-center\r\n              ></v-img>\r\n            </div>\r\n            <p class=\"text-center\">\r\n              Consentimiento informado disminución de estrías <br />\r\n              Fecha: {{ Documento.Fecha }} <br />\r\n            </p>\r\n            Lugar: {{Documento.Lugar}} <br /> \r\n            <br /> El suscrito (a) {{Documento.Nombre}}, en mi condición de usuario, doy mi consentimiento \r\n            informado para que se practique por parte del personal autoirzado del centro de salud, el procedimiento de\r\n            Uvuloplastía\r\n            <br /> <br />\r\n            Por tanto hago constar que de previo a la firma de este consentimiento informado, se han cumplido las siguentes\r\n            condiciones:\r\n             <br /><br />\r\n             1. Recibí información sencilla y completa por parte de un funcionario calificado del centro de salud. En esta,\r\n             se me proporciono información suficiente y comprensible sobre los beneficios y riesgos relacionados con las \r\n             actividades consentidas, los cuales son: <br /><br />\r\n             Riesgos: edema orofaringeo, infección, sangrado, dolor. <br /><br />\r\n             Beneficios: apertura de la vía aérea superior, disminución de ronquidos, disminución de la apnea obstructiva\r\n             del sueño. <br /><br />\r\n             2. Se me ha leído y explicado en forma completa el contenido de este documento, aclarando a la vez,\r\n             todas mis dudas a satisfacción. <br /> <br />\r\n             Aclaro, eso si, que durante mi atención podré cambiar de opinión y cancelar la presente autorización, \r\n             de lo cuál se dejara constancia escrita en el expediente, sin que ello signifique que se me negara los \r\n             restantes servicios de salud.\r\n            <Firma />\r\n          </div> \r\n          <!-- <template>\r\n            <div id=\"firma\">\r\n              <VueSignaturePad\r\n                width=\"500px\"\r\n                height=\"500px\"\r\n                ref=\"signaturePad\"\r\n              />\r\n              <div>\r\n                <button @click=\"save\">Save</button>\r\n                <button @click=\"undo\">Undo</button>\r\n              </div>\r\n            </div>\r\n          </template> -->\r\n\r\n          <!-- <v-card-text class=\"font-weight-medium mt-12 text-center subtitle-1\">\r\n            Especifica los valores para realizar la búsqueda en la columna de la\r\n            izquierda\r\n          </v-card-text> -->\r\n          <div>\r\n            <img v-bind:src=\"Documento.Firma\" />\r\n            <img v-bind:src=\"Documento.Firma2\" />\r\n          </div>\r\n        </v-card>\r\n      </v-hover>\r\n    </v-col>\r\n  </v-row>\r\n</template>\r\n<script>\r\nimport { db } from \"@/main\";\r\nimport Firma from \"./firma.vue\";\r\nexport default {\r\n  components: {\r\n    Firma,\r\n  },\r\n  data: () => ({\r\n    first: \"\",\r\n    last: \"\",\r\n    item: 1,\r\n    Ubicacion: null,\r\n    Nombre: null,\r\n    Suscrito: null,\r\n    Procedimiento: null,\r\n    Profesional: null,\r\n    Riesgos: null,\r\n    Beneficios: null,\r\n    CodigoProfesional: null,\r\n    registro: null,\r\n    elegirdocumento: null,\r\n    Identificacion: null,\r\n    HEntrada: null,\r\n    HSalida: null,\r\n    Autovocher: null,\r\n    Datafonoutilizado: null,\r\n    Impuesto: null,\r\n    Moneda: null,\r\n    Recibido: null,\r\n    Voucher: null,\r\n    Datafono: null,\r\n    name: \"MySignaturePad\",\r\n    items: [\r\n      { text: \"CONSENTIMIENTO INFORMADO\", icon: \"insert_drive_file\" },\r\n      { text: \"Consentimiento Informado\", icon: \"insert_drive_file\" },\r\n      {\r\n        text: \"Consentimiento informado mesoterapia\",\r\n        icon: \"insert_drive_file\"\r\n      },\r\n      { text: \"Justificante Médico\", icon: \"insert_drive_file\" },\r\n      {\r\n        text: \"Consentimiento informado PRP capilar\",\r\n        icon: \"insert_drive_file\"\r\n      },\r\n      { text: \"REINTEGRO DE IVA\", icon: \"insert_drive_file\" },\r\n      { text: \"Consentimiento PRP facial\", icon: \"insert_drive_file\" },\r\n      {\r\n        text: \"Consentimiento informado disminución de estrías\",\r\n        icon: \"insert_drive_file\"\r\n      },\r\n      { text: \"Consentimiento informado Uvuplastia\", icon: \"insert_drive_file\" }\r\n    ],\r\n    Documentos: []\r\n  }),\r\n  created() {\r\n    this.addData();\r\n  },\r\n  methods: {\r\n    // undo() {\r\n    //   this.$refs.signaturePad.undoSignature();\r\n    // },\r\n    // save() {\r\n    //   const { isEmpty, data } = this.$refs.signaturePad.saveSignature();\r\n    //   console.log(isEmpty);\r\n    //   console.log(data);\r\n    // },\r\n    addData() {\r\n      this.Documentos = [];\r\n      this.Documentos.push({\r\n        Lugar: this.Ubicacion,\r\n        Fecha: new Date().toISOString().substr(0, 10),\r\n        Suscrito: this.Suscrito,\r\n        NombreCompleto: this.Nombre,\r\n        Procedimiento: this.Procedimiento,\r\n        Identificacion: this.Identificacion,\r\n        Profesional: this.Profesional,\r\n        Riesgos: this.Riesgos,\r\n        Beneficios: this.Beneficios,\r\n        CodigoProfesional: this.CodigoProfesional,\r\n        HoradeEntrada: this.HEntrada,\r\n        HoradeSalida: this.HSalida,\r\n        Autovocher: this.Autovocher,\r\n        Datafonoutilizado: this.Datafonoutilizado,\r\n        Impuesto: this.Impuesto,\r\n        Moneda: this.Moneda,\r\n        Recibido: this.Recibido,\r\n        Voucher: this.Voucher,\r\n        Datafono: this.Datafono\r\n      });\r\n    },\r\n    async addDocumento() {\r\n      try {\r\n        await db.collection(\"Documentos\").add({\r\n          Lugar: this.Ubicacion,\r\n          Fecha: new Date().toISOString().substr(0, 10),\r\n          Suscrito: this.Suscrito,\r\n          NombreCompleto: this.Nombre,\r\n          Procedimiento: this.Procedimiento,\r\n          Profesional: this.Profesional,\r\n          Riesgos: this.Riesgos,\r\n          Beneficios: this.Beneficios,\r\n          CodigoProfesional: this.CodigoProfesional,\r\n          HoradeEntrada: this.HEntrada,\r\n          HoradeSalida: this.HSalida,\r\n        });\r\n      // this.Ubicacion = \"\";\r\n      // this.Suscrito = \"\";\r\n      // this.Nombre = \"\";\r\n      // this.Procedimiento = \"\";\r\n      // this.Profesional = \"\";\r\n      // this.Riesgos = \"\";\r\n      // this.Beneficios = \"\",\r\n      // this.CodigoProfesional = \"\",\r\n      // this.HoradeEntrada = \"\",\r\n      // this.HoradeSalida = \"\"\r\n      } catch (error) {\r\n        console.log(error);\r\n      }\r\n    },\r\n    created() {\r\n      this.Documentos = [];\r\n      db.collection(\"Documentos\").onSnapshot(res => {\r\n        const changes = res.docChanges();\r\n        changes.forEach(change => {\r\n          if (change.type === \"added\") {\r\n            this.Documentos.push({\r\n              ...change.doc.data(),\r\n              id: change.doc.id\r\n            });\r\n          }\r\n        });\r\n      });\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"sourceRoot":"src/components/Categorias/Documentos"}]}