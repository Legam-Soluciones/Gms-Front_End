AA,QA/BA,sBA+BA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,sBACA,MAAA,CAAA,IAAA,IAAA,MAAA,CAAA,KAAA,IAAA,MAAA,CAAA,GADA;AAAA;AAAA;AAAA;;AAAA;AAAA,uBAEA,EAAA,CAAA,UAAA,CAAA,UAAA,EAAA,GAAA,CAAA;AACA,kBAAA,IAAA,EAAA,MAAA,CAAA,IADA;AAEA,kBAAA,OAAA,EAAA,MAAA,CAAA,OAFA;AAGA,kBAAA,KAAA,EAAA,MAAA,CAAA,KAHA;AAIA,kBAAA,GAAA,EAAA,MAAA,CAAA,GAJA;AAKA,kBAAA,KAAA,EAAA,MAAA,CAAA;AALA,iBAAA,CAFA;;AAAA;AASA,gBAAA,MAAA,CAAA,SAAA;;AACA,gBAAA,MAAA,CAAA,IAAA,GAAA,EAAA,EACA,MAAA,CAAA,OAAA,GAAA,EADA,EAEA,MAAA,CAAA,KAAA,GAAA,EAFA,EAGA,MAAA,CAAA,GAAA,GAAA,EAHA,EAIA,MAAA,CAAA,KAAA,GAAA,EAJA;AAVA;AAAA;;AAAA;AAgBA,gBAAA,KAAA,CAAA,gDAAA,CAAA;;AAhBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAkBA,KAjDA;AAkDA,IAAA,SAlDA,qBAkDA,EAlDA,EAkDA;AACA,WAAA,gBAAA,GAAA,EAAA,CAAA,EAAA;AACA,KApDA;AAqDA,IAAA,WArDA,uBAqDA,EArDA,EAqDA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,EAAA,CAAA,UAAA,CAAA,UAAA,EAAA,GAAA,CAAA,MAAA,CAAA,gBAAA,EAAA,MAAA,CAAA;AACA,kBAAA,OAAA,EAAA,EAAA,CAAA;AADA,iBAAA,CADA;;AAAA;AAIA,gBAAA,MAAA,CAAA,YAAA,GAAA,KAAA,EACA,MAAA,CAAA,gBAAA,GAAA,IADA;;AAJA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAMA,KA3DA;AA4DA,IAAA,WA5DA,uBA4DA,EA5DA,EA4DA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,EAAA,CAAA,UAAA,CAAA,UAAA,EAAA,GAAA,CAAA,EAAA,EAAA,MAAA,EADA;;AAAA;AAEA,gBAAA,MAAA,CAAA,YAAA,GAAA,KAAA,EACA,MAAA,CAAA,SAAA,EADA;;AAFA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAIA,KAhEA;AAiEA,IAAA,SAjEA,4BAiEA;AAAA;;AAAA,UAAA,WAAA,SAAA,WAAA;AAAA,UAAA,KAAA,SAAA,KAAA;;AACA,UAAA,IAAA,GAAA,SAAA,IAAA,GAAA;AACA,QAAA,MAAA,CAAA,aAAA,GAAA,KAAA;AACA,QAAA,MAAA,CAAA,eAAA,GAAA,WAAA,CAAA,MAAA;AACA,QAAA,UAAA,CAAA;AAAA,iBAAA,MAAA,CAAA,YAAA,GAAA,IAAA;AAAA,SAAA,EAAA,EAAA,CAAA;AACA,OAJA;;AAKA,UAAA,KAAA,YAAA,EAAA;AACA,aAAA,YAAA,GAAA,KAAA;AACA,QAAA,UAAA,CAAA,IAAA,EAAA,EAAA,CAAA;AACA,OAHA,MAGA;AACA,QAAA,IAAA;AACA;;AACA,MAAA,WAAA,CAAA,eAAA;AACA,KA9EA;AA+EA,IAAA,WA/EA,8BA+EA;AAAA,UAAA,KAAA,SAAA,KAAA;AAAA,UAAA,GAAA,SAAA,GAAA;AACA,WAAA,KAAA,GAAA,KAAA;AACA,WAAA,GAAA,GAAA,GAAA;AACA,KAlFA;AAmFA,IAAA,GAnFA,eAmFA,CAnFA,EAmFA;AACA,aAAA,CAAA,GAAA,CAAA,IAAA,CAAA,GAAA,EAAA,GACA,IADA,GAEA,CAAA,IAAA,EAAA,IAAA,EAAA,IAAA,EAAA,IAAA,EAAA,IAAA,EAAA,IAAA,EAAA,IAAA,EAAA,IAAA,EAAA,IAAA,EAAA,IAAA,EAAA,CAAA,GAAA,EAAA,CAFA;AAGA;AAvFA;AA1DA,CAAA","sourcesContent":["<template>\r\n  <v-row class=\"fill-height\">\r\n    <v-col>\r\n      <v-sheet height=\"64\">\r\n        <v-toolbar flat color=\"white\">\r\n          <v-btn color=\"primary\" dark @click.stop=\"dialog = true\">\r\n            New Event\r\n          </v-btn>\r\n          <v-btn outlined class=\"mr-4\" @click=\"setToday\">\r\n            Today\r\n          </v-btn>\r\n          <v-btn fab text small @click=\"prev\">\r\n            <v-icon small>mdi-chevron-left</v-icon>\r\n          </v-btn>\r\n          <v-btn fab text small @click=\"next\">\r\n            <v-icon small>mdi-chevron-right</v-icon>\r\n          </v-btn>\r\n          <v-toolbar-title>{{ title }}</v-toolbar-title>\r\n          <div class=\"flex-grow-1\"></div>\r\n          <v-menu bottom right>\r\n            <template v-slot:activator=\"{ on }\">\r\n              <v-btn outlined v-on=\"on\">\r\n                <span>{{ typeToLabel[type] }}</span>\r\n                <v-icon right>mdi-menu-down</v-icon>\r\n              </v-btn>\r\n            </template>\r\n            <v-list>\r\n              <v-list-item @click=\"type = 'day'\">\r\n                <v-list-item-title>Day</v-list-item-title>\r\n              </v-list-item>\r\n              <v-list-item @click=\"type = 'week'\">\r\n                <v-list-item-title>Week</v-list-item-title>\r\n              </v-list-item>\r\n              <v-list-item @click=\"type = 'month'\">\r\n                <v-list-item-title>Month</v-list-item-title>\r\n              </v-list-item>\r\n              <v-list-item @click=\"type = '4day'\">\r\n                <v-list-item-title>4 days</v-list-item-title>\r\n              </v-list-item>\r\n            </v-list>\r\n          </v-menu>\r\n        </v-toolbar>\r\n      </v-sheet>\r\n\r\n      <v-dialog v-model=\"dialog\" max-width=\"500\">\r\n        <v-card>\r\n          <v-container>\r\n            <v-form @submit.prevent=\"addEvent\">\r\n              <v-text-field v-model=\"name\" type=\"text\" label=\"event name (required)\"></v-text-field>\r\n              <v-text-field v-model=\"details\" type=\"text\" label=\"detail\"></v-text-field>\r\n              <v-text-field v-model=\"start\" type=\"date\" label=\"start (required)\"></v-text-field>\r\n              <v-text-field v-model=\"end\" type=\"date\" label=\"end (required)\"></v-text-field>\r\n              <v-text-field v-model=\"color\" type=\"color\" label=\"color (click to open color menu)\"></v-text-field>\r\n              <v-btn type=\"submit\" color=\"primary\" class=\"mr-4\" @click.stop=\"dialog = false\">\r\n                create event\r\n              </v-btn>\r\n            </v-form>\r\n          </v-container>\r\n        </v-card>\r\n      </v-dialog>\r\n\r\n      <v-dialog v-model=\"dialogDate\" max-width=\"500\">\r\n        <v-card>\r\n          <v-container>\r\n            <v-form @submit.prevent=\"addEvent\">\r\n              <v-text-field v-model=\"name\" type=\"text\" label=\"event name (required)\"></v-text-field>\r\n              <v-text-field v-model=\"details\" type=\"text\" label=\"detail\"></v-text-field>\r\n              <v-text-field v-model=\"start\" type=\"date\" label=\"start (required)\"></v-text-field>\r\n              <v-text-field v-model=\"end\" type=\"date\" label=\"end (required)\"></v-text-field>\r\n              <v-text-field v-model=\"color\" type=\"color\" label=\"color (click to open color menu)\"></v-text-field>\r\n              <v-btn type=\"submit\" color=\"primary\" class=\"mr-4\" @click.stop=\"dialog = false\">\r\n                create event\r\n              </v-btn>\r\n            </v-form>\r\n          </v-container>\r\n        </v-card>\r\n      </v-dialog>\r\n\r\n<v-sheet height=\"600\">\r\n  <v-calendar\r\n  ref=\"calendar\"\r\n  v-model=\"focus\"\r\n  color=\"primary\"\r\n  :events=\"events\"\r\n  :event-color=\"getEventColor\"\r\n  :event-margin-bottom=\"3\"\r\n  :now=\"today\"\r\n  :type=\"type\"\r\n  @click:event=\"showEvent\"\r\n  @click:more=\"viewDay\"\r\n  @click:date=\"setDialogDate\"\r\n  @change=\"updateRange\"\r\n  ></v-calendar>\r\n  <v-menu\r\n  v-model=\"selectedOpen\"\r\n  :close-on-content-click=\"false\"\r\n  :activator=\"selectedElement\"\r\n  full-width\r\n  offset-x\r\n  >\r\n  <v-card color=\"grey lighten-4\" :width=\"350\" flat>\r\n    <v-toolbar :color=\"selectedEvent.color\" dark>\r\n      <v-btn @click=\"deleteEvent(selectedEvent.id)\" icon>\r\n        <v-icon>mdi-delete</v-icon>\r\n      </v-btn>\r\n      <v-toolbar-title v-html=\"selectedEvent.name\"></v-toolbar-title>\r\n      <div class=\"flex-grow-1\"></div>\r\n    </v-toolbar>\r\n\r\n    <v-card-text>\r\n      <form v-if=\"currentlyEditing !== selectedEvent.id\">\r\n        {{ selectedEvent.details }}\r\n      </form>\r\n      <form v-else>\r\n        <textarea-autosize\r\n        v-model=\"selectedEvent.details\"\r\n        type=\"text\"\r\n        style=\"width: 100%\"\r\n        :min-height=\"100\"\r\n        placeholder=\"add note\">\r\n      </textarea-autosize>\r\n    </form>\r\n  </v-card-text>\r\n\r\n  <v-card-actions>\r\n    <v-btn text color=\"secondary\" @click=\"selectedOpen = false\">\r\n      close\r\n    </v-btn>\r\n    <v-btn v-if=\"currentlyEditing !== selectedEvent.id\" text @click.prevent=\"editEvent(selectedEvent)\">\r\n      edit\r\n    </v-btn>\r\n    <v-btn text v-else type=\"submit\" @click.prevent=\"updateEvent(selectedEvent)\">\r\n      Save\r\n    </v-btn>\r\n  </v-card-actions>\r\n</v-card>\r\n</v-menu>\r\n</v-sheet>\r\n</v-col>\r\n</v-row>\r\n</template>\r\n\r\n<script>\r\nimport { db } from '@/main'\r\nexport default {\r\n  data: () => ({\r\n    today: new Date().toISOString().substr(0, 10),\r\n    focus: new Date().toISOString().substr(0, 10),\r\n    type: 'month',\r\n    typeToLabel: {\r\n      month: 'Month',\r\n      week: 'Week',\r\n      day: 'Day',\r\n      '4day': '4 Days',\r\n    },\r\n    name: null,\r\n    details: null,\r\n    start: null,\r\n    end: null,\r\n    color: '#1976D2', // default event color\r\n    currentlyEditing: null,\r\n    selectedEvent: {},\r\n    selectedElement: null,\r\n    selectedOpen: false,\r\n    events: [],\r\n    dialog: false,\r\n    dialogDate: false\r\n  }),\r\n  mounted () {\r\n    this.getEvents()\r\n  },\r\n  computed: {\r\n    title () {\r\n      const { start, end } = this\r\n      if (!start || !end) {\r\n        return ''\r\n      }\r\n      const startMonth = this.monthFormatter(start)\r\n      const endMonth = this.monthFormatter(end)\r\n      const suffixMonth = startMonth === endMonth ? '' : endMonth\r\n      const startYear = start.year\r\n      const endYear = end.year\r\n      const suffixYear = startYear === endYear ? '' : endYear\r\n      const startDay = start.day + this.nth(start.day)\r\n      const endDay = end.day + this.nth(end.day)\r\n      switch (this.type) {\r\n        case 'month':\r\n        return `${startMonth} ${startYear}`\r\n        case 'week':\r\n        case '4day':\r\n        return `${startMonth} ${startDay} ${startYear} - ${suffixMonth} ${endDay} ${suffixYear}`\r\n        case 'day':\r\n        return `${startMonth} ${startDay} ${startYear}`\r\n      }\r\n      return ''\r\n    },\r\n    monthFormatter () {\r\n      return this.$refs.calendar.getFormatter({\r\n        timeZone: 'UTC', month: 'long',\r\n      })\r\n    }\r\n  },\r\n  methods: {\r\n    async getEvents () {\r\n      let snapshot = await db.collection('calEvent').get()\r\n      const events = []\r\n      snapshot.forEach(doc => {\r\n        let appData = doc.data()\r\n        appData.id = doc.id\r\n        events.push(appData)\r\n      })\r\n      this.events = events\r\n    },\r\n    setDialogDate( { date }) {\r\n      this.dialogDate = true\r\n      this.focus = date\r\n    },\r\n    viewDay ({ date }) {\r\n      this.focus = date\r\n      this.type = 'day'\r\n    },\r\n    getEventColor (event) {\r\n      return event.color\r\n    },\r\n    setToday () {\r\n      this.focus = this.today\r\n    },\r\n    prev () {\r\n      this.$refs.calendar.prev()\r\n    },\r\n    next () {\r\n      this.$refs.calendar.next()\r\n    },\r\n    async addEvent () {\r\n      if (this.name && this.start && this.end) {\r\n        await db.collection(\"calEvent\").add({\r\n          name: this.name,\r\n          details: this.details,\r\n          start: this.start,\r\n          end: this.end,\r\n          color: this.color\r\n        })\r\n        this.getEvents()\r\n        this.name = '',\r\n        this.details = '',\r\n        this.start = '',\r\n        this.end = '',\r\n        this.color = ''\r\n      } else {\r\n        alert('You must enter event name, start, and end time')\r\n      }\r\n    },\r\n    editEvent (ev) {\r\n      this.currentlyEditing = ev.id\r\n    },\r\n    async updateEvent (ev) {\r\n      await db.collection('calEvent').doc(this.currentlyEditing).update({\r\n        details: ev.details\r\n      })\r\n      this.selectedOpen = false,\r\n      this.currentlyEditing = null\r\n    },\r\n    async deleteEvent (ev) {\r\n      await db.collection(\"calEvent\").doc(ev).delete()\r\n      this.selectedOpen = false,\r\n      this.getEvents()\r\n    },\r\n    showEvent ({ nativeEvent, event }) {\r\n      const open = () => {\r\n        this.selectedEvent = event\r\n        this.selectedElement = nativeEvent.target\r\n        setTimeout(() => this.selectedOpen = true, 10)\r\n      }\r\n      if (this.selectedOpen) {\r\n        this.selectedOpen = false\r\n        setTimeout(open, 10)\r\n      } else {\r\n        open()\r\n      }\r\n      nativeEvent.stopPropagation()\r\n    },\r\n    updateRange ({ start, end }) {\r\n      this.start = start\r\n      this.end = end\r\n    },\r\n    nth (d) {\r\n      return d > 3 && d < 21\r\n      ? 'th'\r\n      : ['th', 'st', 'nd', 'rd', 'th', 'th', 'th', 'th', 'th', 'th'][d % 10]\r\n    }\r\n  }\r\n}\r\n</script>"],"sourceRoot":"src/components/Categorias/Agenda"}]}